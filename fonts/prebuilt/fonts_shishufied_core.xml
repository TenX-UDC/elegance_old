<fonts-modification version="1">
    <family customizationType="new-named-family" name="gobold-sys">
        <font weight="300" style="normal">GoboldThinLight.ttf</font>
        <font weight="300" style="italic">GoboldThinLight-Italic.ttf</font>
        <font weight="400" style="normal">Gobold.ttf</font>
        <font weight="400" style="italic">Gobold-Italic.ttf</font>
        <font weight="700" style="normal">GoboldBold.ttf</font>
        <font weight="700" style="italic">GoboldBold-Italic.ttf</font>
    </family>

    <alias name="gobold-light-sys" to="gobold-sys" weight="300" />
</fonts-modification>
